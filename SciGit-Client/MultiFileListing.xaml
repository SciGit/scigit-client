<UserControl x:Class="SciGit_Client.MultiFileListing"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:SciGit_Client="clr-namespace:SciGit_Client" mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300" SnapsToDevicePixels="True">
    <StackPanel>
        <SciGit_Client:FileListing x:Name="updatedListing" Title="Updated Files" Color="#9999ff" Margin="0,0,0,10" />
        <SciGit_Client:FileListing x:Name="createdListing" Title="Created Files" Color="#55ff55" Margin="0,0,0,10" />
        <SciGit_Client:FileListing x:Name="deletedListing" Title="Deleted Files" Color="#ff5555" />
    </StackPanel>
</UserControl>
